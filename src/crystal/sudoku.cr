def sd_genmat()
	mr = Array.new(324) { Array.new(9) { 0_u16 } }
	mc = Array.new(729) { Array.new(4) { 0_u16 } }
	r = 0
	(0_u16...9_u16).each do |i|
		(0_u16...9_u16).each do |j|
			(0_u16...9_u16).each do |k|
				mc[r][0] = (9_u16 * i + j).as(UInt16)
				mc[r][1] = ((i // 3_u16 * 3_u16 + j // 3_u16) * 9_u16 + k + 81_u16).as(UInt16)
				mc[r][2] = (9_u16 * i + k + 162_u16).as(UInt16)
				mc[r][3] = (9_u16 * j + k + 243_u16).as(UInt16)
				r += 1
			end
		end
	end
	nr = Array.new(324) { 0_i8 }
	(0_u16...729_u16).each do |r|
		(0_u16...4_u16).each do |c2|
			k = mc[r][c2]
			mr[k][nr[k]] = r.as(UInt16)
			nr[k] += 1
		end
	end
	return mr, mc
end

def sd_update(mr : Array(Array(UInt16)), mc : Array(Array(UInt16)), sr : Array(Int8), sc : Array(UInt8), r : Int, v : Int)
	min, min_c = 10, 0
	(0...4).each do |c2|
		if v > 0
			sc[mc[r][c2]] |= 0x80 
		else sc[mc[r][c2]] &= 0x7F end
	end
	(0...4).each do |c2|
		c = mc[r][c2]
		if v > 0
			(0...9).each do |r2|
				rr = mr[c][r2]
				sr[rr] += 1
				if sr[rr] == 1
					p = mc[rr]
					sc[p[0]] -= 1; sc[p[1]] -= 1; sc[p[2]] -= 1; sc[p[3]] -= 1
					if sc[p[0]] < min
						min, min_c = sc[p[0]], p[0]
					end
					if sc[p[1]] < min
						min, min_c = sc[p[1]], p[1]
					end
					if sc[p[2]] < min
						min, min_c = sc[p[2]], p[2]
					end
					if sc[p[3]] < min
						min, min_c = sc[p[3]], p[3]
					end
				end
			end
		else
			(0...9).each do |r2|
				rr = mr[c][r2]
				sr[rr] -= 1
				if sr[rr] == 0
					p = mc[rr]
					sc[p[0]] += 1; sc[p[1]] += 1; sc[p[2]] += 1; sc[p[3]] += 1
				end
			end
		end
	end
	return min, min_c
end

def sd_solve(mr : Array(Array(UInt16)), mc : Array(Array(UInt16)), s : String)
	sr, sc, hints = Array.new(729) { 0_i8 }, Array.new(324) { 9_u8 }, 0
	(0...81).each do |i|
		a = -1
		if s[i] >= '1' && s[i] <= '9'
			a = s[i].ord - 49
		end
		if a >= 0
			sd_update(mr, mc, sr, sc, i * 9 + a, 1)
			hints += 1
		end
	end
	cr, cc = Array.new(81) { -1_i8 }, Array.new(81) { 0_i16 }
	i, min, dir = 0, 10, 1
	while true
		while i >= 0 && i < 81 - hints
			if dir == 1
				if min > 1
					(0...324).each do |c|
						if sc[c] < min
							min, cc[i] = sc[c], c.as(Int16)
							if min < 2
								break
							end
						end
					end
				end
				if min == 0 || min == 10
					cr[i], dir, i = -1, -1, i - 1
				end
			end
			c = cc[i]
			if dir == -1 && cr[i] >= 0
				sd_update(mr, mc, sr, sc, mr[c][cr[i]], -1)
			end
			r2 = cr[i] + 1
			while r2 < 9 && sr[mr[c][r2]] != 0
				r2 += 1
			end
			if r2 < 9
				min, cc[i+1] = sd_update(mr, mc, sr, sc, mr[c][r2], 1)
				cr[i], dir, i = r2, 1, i + 1
			else
				cr[i], dir, i = -1, -1, i - 1
			end
		end
		if i < 0
			break
		end
		o = Array.new(81) { 49u8 }
		(0...81).each do |j|
			o[j] = s[j].ord - 48
		end
		(0...i).each do |j|
			r = mr[cc[j]][cr[j]]
			o[r/9] = r % 9 + 1
		end
		puts String.new(o)
		i, dir = i - 1, -1
	end
end

